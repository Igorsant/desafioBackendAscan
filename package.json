{
  "name": "rabbitmq-tutorial",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "yarn migrate:up && ts-node ./src/index.ts",
    "start:dev": "nodemon",
    "migrate:up": "knex migrate:up --knexfile src/knexfile",
    "migrate:down": "knex migrate:down --knexfile src/knexfile",
    "seed:run": "knex seed:run --knexfile src/knexfile",
    "send-notifications": "ts-node ./src/sendNotifications.ts"
  },
  "dependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "amqplib": "^0.10.0",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "knex": "^2.2.0",
    "nodemon": "^2.0.19",
    "pg": "^8.7.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "prettier": "2.7.1"
  }
}
